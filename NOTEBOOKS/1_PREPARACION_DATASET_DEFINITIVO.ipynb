    "# Definir funci√≥n para asignar franja\n",
    "def asignar_franja(dias):\n",
    "    if dias < 1:\n",
    "        return \"0-24h\"\n",
    "    elif dias < 2:\n",
    "        return \"24-48h\"\n",
    "    elif dias < 3:\n",
    "        return \"48-72h\"\n",
    "    elif dias < 4:\n",
    "        return \"72-96h\"\n",
    "    else:\n",
    "        return \"96h+\"\n",
    "\n",
    "# Crear columna de franja\n",
    "df[\"franja_trending\"] = df[\"video_age_days\"].apply(asignar_franja)\n",
    "\n",
    "# Filtrar solo v√≠deos mostpopular\n",
    "df_mp = df[df[\"mostpopular\"] == 1]\n",
    "\n",
    "# Contar y calcular porcentajes\n",
    "dist = df_mp[\"franja_trending\"].value_counts(normalize=True).sort_index() * 100\n",
    "\n",
    "print(\"Distribuci√≥n de entrada a trending por franja (en %):\")"
